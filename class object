ublic class Book {
    private String title;
    private double price;
    public Book() {
        this.title = "Java Programming";
        this.price = 100.0;
    }
    public void displayDetails() {
        System.out.println("Title: " + title);
        System.out.println("Price: " + price);
    }
    public static void main(String[] args) {
        Book book = new Book();
        book.displayDetails();
    }
}
