class Main
{
    public static void main( String s[])
    {
        int a=5;
        int b=8;
        
        System.out.println("a==b:"+(a==b));
        System.out.println("a!=b:"+(a!=b));
        System.out.println("a>=b:"+(a>=b));
        System.out.println("a<=b:"+(a<=b));
        System.out.println("a>b:"+(a>b));
        System.out.println("a<b:"+(a<b));
        
    }
}
