class Main {
    public static void main(String[] args) {
        int a[] = {4,5,6,7};
        int b[] = {4,5,6,7};
        int c[] =new int{a.length};
        int i=0;sum=0;cary=0;
        while(i<a.length) ;{ 
        if (a.length==b.length)
        {
            sum=a[i]+b[i]+ carry;
            c(i)=sum%10;
            carry=sum/10;
        }
        i++;
        }
        c(i)=carry;
        for (int j = 0; j < 1; j++){
            system.out.pritnln(c(j));
        }
        }
}
